# PySpark cell:

from pyspark.sql.functions import col

silver_df = spark.table("silver.encounter")

clean_df = (
    silver_df
    .withColumnRenamed("subject_reference", "patient_id")
    .filter(col("patient_id").isNotNull())
)

clean_df.display()
