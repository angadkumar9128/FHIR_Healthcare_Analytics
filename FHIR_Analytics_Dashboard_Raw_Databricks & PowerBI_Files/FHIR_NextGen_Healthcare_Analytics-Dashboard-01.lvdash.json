{
  "datasets": [
    {
      "name": "89eab4eb",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `angad_kumar91`;\n",
        "USE SCHEMA `fhir_healthcare_analytics_gold`;\n",
        "\n",
        "SELECT\n",
        "  admission_date,\n",
        "  total_admissions\n",
        "FROM angad_kumar91.fhir_healthcare_analytics_gold.patient_admissions\n",
        "ORDER BY admission_date;\n"
      ]
    },
    {
      "name": "4b0151c8",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `angad_kumar91`;\n",
        "USE SCHEMA `fhir_healthcare_analytics_gold`;\n",
        "\n",
        "SELECT\n",
        "  diagnosis,\n",
        "  patient_count\n",
        "FROM angad_kumar91.fhir_healthcare_analytics_gold.disease_patterns\n",
        "ORDER BY patient_count DESC\n",
        "LIMIT 15;\n"
      ]
    },
    {
      "name": "cd27fe77",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `angad_kumar91`;\n",
        "USE SCHEMA `fhir_healthcare_analytics_gold`;\n",
        "\n",
        "SELECT\n",
        "  diagnosis,\n",
        "  patient_count,\n",
        "  encounter_count\n",
        "FROM angad_kumar91.fhir_healthcare_analytics_gold.disease_patterns\n",
        "ORDER BY encounter_count DESC;\n"
      ]
    },
    {
      "name": "72f4caa8",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `angad_kumar91`;\n",
        "USE SCHEMA `fhir_healthcare_analytics_gold`;\n",
        "\n",
        "SELECT\n",
        "  medication_name,\n",
        "  prescription_count\n",
        "FROM angad_kumar91.fhir_healthcare_analytics_gold.medication_trends\n",
        "ORDER BY prescription_count DESC\n",
        "LIMIT 15;\n"
      ]
    },
    {
      "name": "2c70da9b",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `angad_kumar91`;\n",
        "USE SCHEMA `fhir_healthcare_analytics_gold`;\n",
        "\n",
        "SELECT\n",
        "  observation_name,\n",
        "  DATE(admit_time) AS observation_date,\n",
        "  AVG(observation_value) AS avg_value\n",
        "FROM angad_kumar91.fhir_healthcare_analytics_gold.patient_encounter_summary\n",
        "WHERE observation_value IS NOT NULL\n",
        "GROUP BY observation_name, DATE(admit_time)\n",
        "ORDER BY observation_date;\n"
      ]
    },
    {
      "name": "cc4ae92f",
      "displayName": "Untitled dataset",
      "queryLines": [
        "USE CATALOG `angad_kumar91`;\n",
        "USE SCHEMA `fhir_healthcare_analytics_gold`;\n",
        "\n",
        "SELECT\n",
        "  procedure_name,\n",
        "  procedure_count\n",
        "FROM angad_kumar91.fhir_healthcare_analytics_gold.procedure_trends\n",
        "ORDER BY procedure_count DESC\n",
        "LIMIT 15;\n"
      ]
    }
  ],
